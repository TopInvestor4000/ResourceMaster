@page "/my-table"
@using ResourceMaster.Data
@inject DatabaseContext DatabaseContext

<h1>My Table</h1>

<table>
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Age</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in DatabaseContext.MyTables)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Age</td>
            </tr>
        }
    </tbody>
</table>
